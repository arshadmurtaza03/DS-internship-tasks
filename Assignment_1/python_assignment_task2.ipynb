        "# Running Sum of 1d Array"
      ],
      "metadata": {
        "id": "L1qKd8tgfjO2"
      }
    },
    {
      "cell_type": "code",
      "execution_count": 4,
      "metadata": {
        "id": "X_h0exBEfenu"
      },
      "outputs": [],
      "source": [
        "class Solution:\n",
        "    def runningSum(self, nums: List[int]) -> List[int]:\n",
        "        total = 0\n",
        "        result = []\n",
        "\n",
        "        for value in nums:\n",
        "            total += value\n",
        "            result.append(total)\n",
        "\n",
        "        return result"
      ]
    },
